<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.material.dao.MaterialDao">

    <select id="getMaterialOutSheetById" resultType="com.example.material.entity.MaterialOutSheet">
        select * from MATERIAL_OUTSHEETS where OUTSHEETNO=#{id}
    </select>
</mapper>